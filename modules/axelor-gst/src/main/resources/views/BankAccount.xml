<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

  <grid model="com.axelor.apps.gst.db.BankAccount"
    title="Bank Account Grid" name="bank-account-grid">
    <field name="bank" />
    <field name="accountNumber" />
    <field name="ifsc" />
    <field name="type" />
    <field name="address" />
  </grid>

  <form model="com.axelor.apps.gst.db.BankAccount"
    title="Bank Account Form" name="bank-account-form">
    <panel colSpan="12">
      <field name="bank" />
      <field name="accountNumber" />
      <field name="ifsc" />
      <field name="type" />
      <field name="address" />
    </panel>
  </form>
</object-views>
