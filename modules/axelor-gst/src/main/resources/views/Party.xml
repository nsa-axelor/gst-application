<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

  <grid model="com.axelor.apps.gst.db.Party" title="Party Grid"
    name="party-grid">
    <field name="reference" />
    <field name="name" />
    <field name="type" />
    <field name="isCustomer" />
    <field name="isSupplier" />
    <field name="gstin" />
    <field name="contactList" />
    <field name="addressList" />
  </grid>

  <form model="com.axelor.apps.gst.db.Party" title="Party Form"
    name="party-form">
    <panel>
      <field name="reference" colSpan="12" />
      <field name="name" />
      <field name="type" />
      <field name="gstin" />
    </panel>
    <panel>
      <field name="isCustomer" />
      <field name="isSupplier" />
    </panel>
    <panel>
      <field name="contactList" colSpan="12"/>
      <field name="addressList" colSpan="12"/>
    </panel>

  </form>

</object-views>
