<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

    <grid model="com.axelor.apps.gst.db.Sequence" title="Sequence Grid" name="sequence-grid">
        <field name="prefix" />
        <field name="suffix" />
        <field name="padding" />
        <field name="nextNumber" />
        <field name="model" />
    </grid>

    <form model="com.axelor.apps.gst.db.Sequence" title="Sequence Form" name="sequence-form">
        <panel>
            <field name="prefix" />
            <field name="suffix" />
            <field name="padding" />
            <field name="nextNumber" />
            <field name="model" />
        </panel>

    </form>

</object-views>
